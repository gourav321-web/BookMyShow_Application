<!-- app/views/sessions/new.html.erb -->
<h1>Login_Form</h1>

<%= form_with url: login_path do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email, placeholder: 'Enter your email' %>
  </div>

  <div>
    <%= form.label :password %>
    <%= form.password_field :password, placeholder: 'Enter your password' %>
  </div>

  <div>
    <%= form.submit 'Log In' %>
  </div>
<% end %>
